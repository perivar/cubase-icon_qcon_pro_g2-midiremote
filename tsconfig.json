{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "midiremote_api_v1": [".api/v1"]
    },
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "checkJs": true,
    "lib": ["es5"],
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "strict": false,
    "noEmit": false,
    "skipLibCheck": true,
    "outDir": "dist"
    // "outFile": "dist/icon/qcon_pro_g2/icon_qcon_pro_g2.js"
  },
  "include": ["src/"],
  "exclude": ["src/**/*.spec.ts", "src/**/*.test.ts"]
}
