{
  "name": "cubase-icon_qcon_pro_g2-midiremote",
  "version": "1.0.0",
  "description": "Cubase 12 MIDI Remote Script for the Icon QCon Pro G2 and Extender",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/perivar/cubase-icon_qcon_pro_g2-midiremote.git"
  },
  "scripts": {
    "postinstall": "patch-package",
    "go": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./src/index.ts",
    "tsc": "tsc --project ./tsconfig-es5.json",
    "tsc-clean": "tsc --build ./tsconfig-es5.json --clean",
    "tsc2021": "tsc --project ./tsconfig.json",
    "tsc2021-clean": "tsc --build ./tsconfig.json --clean",
    "clean": "rimraf dist",
    "tsup": "tsup",
    "start": "tsup --watch",
    "webpack": "webpack --mode production",
    "rollup": "rollup -c --environment BUILD:production --bundleConfigAsCjs",
    "rollup-watch": "rollup -c -w --environment BUILD:development --bundleConfigAsCjs",
    "post": "ts-node ./postinstall.ts",
    "post-and-fix": "yarn post && yarn lint-fix",
    "build": "yarn clean && yarn tsc && yarn webpack && yarn post && yarn tsup && yarn rollup && yarn lint-fix",
    "lint": "eslint \"**/*.{mjs,js,ts}\"",
    "lint-fix": "eslint \"**/*.{mjs,js,ts}\" --fix",
    "format": "prettier --check \"**/*.{mjs,js,ts}\"",
    "format-fix": "prettier --write \"**/*.{mjs,js,ts}\"",
    "dev": "tsc-watch --noClear -p ./tsconfig-es5.json --onSuccess \"yarn post-and-fix\"",
    "test": "jest",
    "go-webpack": "ts-node -r tsconfig-paths/register dist/icon/qcon_pro_g2/icon_qcon_pro_g2_webpack.js",
    "go-tsup": "ts-node -r tsconfig-paths/register dist/icon/qcon_pro_g2/icon_qcon_pro_g2_tsup.js",
    "go-rollup": "ts-node -r tsconfig-paths/register dist/icon/qcon_pro_g2/icon_qcon_pro_g2_rollup.js"
  },
  "author": "perivar",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@swc/core": "^1.3.62",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "core-js": "^3.30.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "execa": "^7.1.1",
    "jest": "^29.5.0",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prepend-file": "^2.0.1",
    "prettier": "^2.8.8",
    "remove-comments-loader": "^0.1.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.1.3",
    "winston": "^3.9.0"
  }
}
