{
    "name": "cubase-icon_qcon_pro_g2-midiremote",
    "version": "1.0.0",
    "description": "Cubase 12 MIDI Remote Script for the Icon QCon Pro G2 and Extender",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/perivar/cubase-icon_qcon_pro_g2-midiremote.git"
    },
    "scripts": {
        "tsc": "tsc",
        "webpack": "webpack --mode production",
        "build": "yarn tsc && yarn webpack && yarn clean && yarn lint-fix",
        "clean": "ts-node ./postinstall.ts",
        "clean-and-fix": "yarn clean && yarn lint-fix",
        "lint": "eslint \"**/*.{js,ts}\"",
        "lint-fix": "eslint \"**/*.{js,ts}\" --fix",
        "format": "prettier --check \"**/*.{js,ts}\"",
        "format-fix": "prettier --write \"**/*.{js,ts}\"",
        "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"yarn clean-and-fix\"",
        "test": "jest"
    },
    "author": "perivar",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^29.5.1",
        "@types/node": "^20.1.1",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jest": "^29.5.0",
        "prepend-file": "^2.0.1",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.0.4",
        "webpack": "^5.82.1",
        "webpack-cli": "^5.1.1",
        "webpack-node-externals": "^3.0.0"
    }
}
