{
    "name": "cubase-icon_qcon_pro_g2-midiremote",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "node build.mjs",
        "start": "node build.mjs --watch",
        "tsc": "tsc",
        "clean": "ts-node ./postinstall.ts",
        "clean-and-fix": "yarn clean && yarn lint-fix",
        "postversion": "npm run build",
        "postinstall": "patch-package",
        "lint": "eslint \"**/*.{js,ts}\"",
        "lint-fix": "eslint \"**/*.{js,ts}\" --fix",
        "format": "prettier --check \"**/*.{js,ts}\"",
        "format-fix": "prettier --write \"**/*.{js,ts}\"",
        "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"yarn clean-and-fix\"",
        "test": "jest"
    },
    "author": "perivar",
    "license": "MIT",
    "dependencies": {
        "abbreviate": "^0.0.3",
        "color-diff": "^1.3.0",
        "core-js": "^3.30.2"
    },
    "devDependencies": {
        "@bjoluc/semantic-release-config-npm": "^4.0.0",
        "@swc/core": "^1.3.57",
        "@types/color-diff": "^1.2.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.1.1",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "dotenv": "^16.0.3",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "execa": "^7.1.1",
        "jest": "^29.5.0",
        "patch-package": "^7.0.0",
        "prepend-file": "^2.0.1",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.4",
        "tsup": "^6.7.0",
        "type-fest": "^3.10.0",
        "typescript": "^5.0.4"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    }
}
