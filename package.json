{
    "name": "cubase-icon_qcon_pro_g2-midiremote",
    "version": "1.0.0",
    "description": "Cubase 12 MIDI Remote Script for the Icon QCon Pro G2 and Extender",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/perivar/cubase-icon_qcon_pro_g2-midiremote.git"
    },
    "scripts": {
        "go": "NODE_ENV=development ts-node -r tsconfig-paths/register ./src/index.ts",
        "tsup": "node tsup.mjs",
        "start": "node tsup.mjs --watch",
        "tsc": "tsc",
        "webpack": "webpack --mode production",
        "build": "yarn tsc && yarn webpack && yarn clean && yarn lint-fix",
        "clean": "ts-node ./postinstall.ts",
        "clean-and-fix": "yarn clean && yarn lint-fix",
        "lint": "eslint \"**/*.{js,ts}\"",
        "lint-fix": "eslint \"**/*.{js,ts}\" --fix",
        "format": "prettier --check \"**/*.{js,ts}\"",
        "format-fix": "prettier --write \"**/*.{js,ts}\"",
        "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"yarn clean-and-fix\"",
        "test": "jest",
        "go-webpack": "ts-node -r tsconfig-paths/register dist/icon/qcon_pro_g2/icon_qcon_pro_g2_webpack.js",
        "go-tsup": "ts-node -r tsconfig-paths/register dist/icon/qcon_pro_g2/icon_qcon_pro_g2_tsup.js"
    },
    "author": "perivar",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@swc/core": "^1.3.58",
        "@types/jest": "^29.5.1",
        "@types/node": "~16.0.0",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "core-js": "^3.30.2",
        "dotenv": "^16.0.3",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-es5": "^1.5.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "execa": "^7.1.1",
        "jest": "^29.5.0",
        "prepend-file": "^2.0.1",
        "prettier": "^2.8.8",
        "remove-comments-loader": "^0.1.2",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.4",
        "tsconfig-paths": "^4.2.0",
        "tsup": "^6.7.0",
        "typescript": "^5.0.4",
        "webpack": "^5.83.1",
        "webpack-cli": "^5.1.1",
        "winston": "^3.8.2"
    }
}
